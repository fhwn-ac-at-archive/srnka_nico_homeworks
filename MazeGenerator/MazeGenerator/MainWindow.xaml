<Window x:Class="MazeGenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MazeGenerator"
        mc:Ignorable="d"
        Title="MainWindow" Height="329.289" Width="334.674">
    <Grid>
        <StackPanel Margin="0,20">
            <Label HorizontalAlignment="Center">Width:</Label>
            <TextBox x:Name="WidthBox" Height="28"></TextBox>
            <Label HorizontalAlignment="Center">Height:</Label>
            <TextBox x:Name="HeightBox" Height="28"></TextBox>
            <Label HorizontalAlignment="Center">Canvas Width:</Label>
            <TextBox x:Name="CanvasWidth" Height="28"></TextBox>
            <Label HorizontalAlignment="Center">Canvas Height:</Label>
            <TextBox x:Name="CanvasHeight" Height="28"></TextBox>
            <Button x:Name="Start" Height="28" Click="Start_Click">Generate</Button>
        </StackPanel>
    </Grid>

</Window>
